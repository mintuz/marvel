{
    "name": "@mintuz/marvel",
    "version": "1.0.0",
    "description": "A collection of reusable UI components built in React",
    "main": "dist/index.js",
    "scripts": {
        "check-types": "tsc --project tsconfig.json --noEmit",
        "check-types:watch": "yarn check-types -- --watch",
        "prettier": "prettier",
        "ui-automation": "cypress run",
        "dev": "start-storybook -p 6006",
        "build-storybook": "build-storybook --docs",
        "babel": "babel src --extensions \".ts,.tsx,.js,.jsx\" --out-dir dist",
        "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
        "build": "yarn babel && yarn build:types"
    },
    "author": "Adam Bulmer <github@mintuz.com>",
    "license": "MIT",
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "styled-components": "^5.0.0"
    },
    "dependencies": {
        "@mintuz/react-intersect": "^1.1.0",
        "@styled-system/css": "^5.1.5",
        "@styled-system/theme-get": "^5.1.2",
        "classnames": "^2.3.1",
        "polished": "^3.4.1",
        "prop-types": "^15.8.1",
        "styled-icons": "^9.2.0",
        "styled-system": "^5.1.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.9",
        "@babel/node": "^7.16.8",
        "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
        "@babel/plugin-transform-classes": "^7.16.7",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@cypress/webpack-preprocessor": "^4.1.1",
        "@mintuz/marvel-tsconfig": "*",
        "@storybook/addon-actions": "^5.3.9",
        "@storybook/addon-docs": "^5.3.9",
        "@storybook/addon-links": "^5.3.9",
        "@storybook/addons": "^5.3.9",
        "@storybook/preset-typescript": "^1.2.0",
        "@storybook/react": "^5.3.9",
        "@storybook/storybook-deployer": "^2.8.1",
        "@testing-library/cypress": "^4.0.4",
        "@types/classnames": "^2.3.1",
        "@types/styled-components": "^5.1.25",
        "@types/styled-system": "^5.1.15",
        "@types/styled-system__css": "^5.0.16",
        "@types/styled-system__theme-get": "^5.0.2",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.2.4",
        "cypress": "^3.8.2",
        "cypress-react-unit-test": "^2.4.3",
        "prettier": "1.19.1",
        "react": "^16.12.0",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.12.0",
        "styled-components": "^5.3.5",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3"
    }
}
