{
  "name": "@mintuz/marvel",
  "version": "1.0.3",
  "description": "A collection of reusable UI components built in React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "check:types": "tsc --project tsconfig.json --noEmit",
    "check:types:watch": "yarn check-types -- --watch",
    "prettier": "prettier",
    "rollup": "rollup -c",
    "build:types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "build": "yarn rollup && yarn build:types",
    "build:watch": "rollup -c -w",
    "lint": "eslint ./src --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage"
  },
  "author": "Adam Bulmer <github@mintuz.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "16 - 18",
    "react-dom": "16 - 18",
    "styled-components": "^5.0.0"
  },
  "dependencies": {
    "@mintuz/marvel-theme": "^1.0.0",
    "@radix-ui/react-accordion": "^0.1.6",
    "classnames": "^2.3.1",
    "styled-icons": "^10.45.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@mintuz/marvel-eslint-preset": "*",
    "@mintuz/marvel-tsconfig": "*",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/classnames": "^2.3.1",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/styled-system__css": "^5.0.16",
    "@types/styled-system__theme-get": "^5.0.2",
    "@vitejs/plugin-react": "^1.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^2.70.2",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.6.3",
    "vitest": "^0.10.5"
  }
}
